{
  "manifest_version": 2,
  "name": "__MSG_app_name__ Alt.Icon",
  "version": "3.3.1.1",
  "description": "__MSG_app_slogan__ -- An alternative icon version/fork (**CAUTION**: visit 'Homepage' bellow). --",
  "homepage_url": "https://github.com/Gregory-K/Markdown-Here.Alt.Icon",
  "applications": {
    "gecko": {
      "id": "markdown-here-revival@xul.calypsoblue.org",
      "strict_min_version": "78.5.0",
      "strict_max_version": "102.0"
    }
  },
  "default_locale": "en",
  "icons": {
    "16": "images/md_bw.svg",
    "32": "images/md_fucsia.svg",
    "48": "images/md_fucsia.svg",
    "128": "images/md_fucsia.svg",
    "512": "images/md_fucsia.svg"
  },
  "permissions": ["compose", "menus", "storage", "tabs"],
  "background": {"page": "background.html"},
  "compose_action": {
    "browser_style": true,
    "default_area": "formattoolbar",
    "default_icon": {
      "16": "images/md_bw.svg",
      "19": "images/md_bw.svg",
      "32": "images/md_fucsia.svg",
      "64": "images/md_fucsia.svg"
    },
    "theme_icons": [
      {
        "dark": "images/md_dark.svg",
        "light": "images/md_light.svg",
        "size": 16
      },
      {
        "dark": "images/md_dark.svg",
        "light": "images/md_light.svg",
        "size": 19
      },
      {
        "dark": "images/md_fucsia.svg",
        "light": "images/md_fucsia.svg",
        "size": 32
      },
      {
        "dark": "images/md_fucsia.svg",
        "light": "images/md_fucsia.svg",
        "size": 64
      }
    ],
    "default_title": "__MSG_toggle_button_tooltip__"
  },
  "commands": {
    "toggle-markdown": {
      "suggested_key": {
        "default": "Ctrl+Alt+M",
        "mac": "MacCtrl+Ctrl+M"
      },
      "description": "__MSG_toggle_button_tooltip__"
    }
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },
  "experiment_apis": {
    "notificationbar": {
      "schema": "experiments/notificationbar_schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "paths": [["notificationbar"]],
        "script": "experiments/notificationbar.js"
      }
    }
  }
}
